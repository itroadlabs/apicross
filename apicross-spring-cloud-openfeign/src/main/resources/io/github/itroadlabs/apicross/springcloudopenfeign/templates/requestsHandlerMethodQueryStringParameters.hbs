{{#each queryParameters}}
    {{#with type}}
     @RequestParam(
                  name="{{{../name}}}",
                  required={{../required}}
                  {{#if defaultValue}}
                      {{#if primitive}}
                          , defaultValue = "{{{defaultValue}}}"
                      {{/if}}
                  {{/if}}
                   ) {{> typeWithoutListConstraints}} {{../resolvedName}} ,
    {{/with}}
{{/each}}