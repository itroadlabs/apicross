{{#each queryParameters}}
    {{#with type}}
     @RequestParam(
                  name="{{{../name}}}",
                  required={{../required}}
                  {{#unless ../required}}
                      {{#if defaultValue}}
                          {{#if primitive}}
                              , defaultValue = "{{{defaultValue}}}"
                          {{/if}}
                      {{/if}}
                  {{/unless}}
                   ) {{> typeWithoutListConstraints}} {{../resolvedName}} ,
    {{/with}}
{{/each}}